{
    "name": "vscode-neos-fusion-grammar",
    "displayName": "Neos Fusion & AFX Grammar",
    "description": "Syntax highlighting for Neos Fusion (and AFX)",
    "version": "0.1.0",
    "publisher": "SimonSchmidt",
    "icon": "images/logo.png",
    "galleryBanner": {
        "color": "#175b4f",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.70.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "eel",
                "aliases": [
                    "EEL"
                ],
                "extensions": [
                    ".eel"
                ]
            },
            {
                "id": "fusion",
                "aliases": [
                    "Fusion",
                    "fusion"
                ],
                "icon": {
                    "dark": "./images/filetype_dark.svg",
                    "light": "./images/filetype_light.svg"
                },
                "extensions": [
                    ".fusion"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "eel",
                "scopeName": "source.eel",
                "path": "./syntaxes/eel.tmLanguage.json"
            },
            {
                "language": "fusion",
                "scopeName": "source.fusion",
                "path": "./syntaxes/fusion.tmLanguage.json"
            }
        ]
    },
    "repository": "git@github.com:sjsone/vscode-neos-fusion-grammar.git",
    "bugs": {
        "url": "https://github.com/sjsone/vscode-neos-fusion-grammar/issues"
    },
    "homepage": "https://github.com/sjsone/vscode-neos-fusion-grammar#readme",
    "author": "Simon J. Schmidt",
    "license": "AGPL-3.0-or-later",
    "keywords": [
        "Neos",
        "Neos CMS",
        "Fusion",
        "AFX",
        "EEL"
    ]
}